  <d2d:ArticleRenderDef rdf:about="#This">
    <rdfs:label>D2D Rendering Defintion for a Person Profile article.</rdfs:label>
    <d2d:renders rdf:resource="/d2d/ad/PersonProfile#PersonProfile" />

    <d2d:hasTemplate>
      <![CDATA[
          <d2d:Template xmlns:d2d="http://rdfns.org/d2d/">
            <article style="margin-left: 0px; padding-top: 40px; padding-left: 5px;">
              <div class="personprofile">
                <d2d:Field d2d:index="1"><a href="[d2d:Content /]" target="_blank"><h4>[/d2d:Field]<d2d:Field />[d2d:Field d2d:index="1"]</h4></a></d2d:Field>
                 <span>
                  <d2d:Field><p>Birth: <d2d:Content /><d2d:Field> (<d2d:Content />)</d2d:Field></p></d2d:Field>
                  <d2d:Field><p>Death: <d2d:Content /><d2d:Field> (<d2d:Content />)</d2d:Field></p></d2d:Field>
                  <d2d:Field><p>Alma Mater: <d2d:Content /></p></d2d:Field>
                  <d2d:Field><img src="[d2d:Content /]" /></d2d:Field>
                  <d2d:Field><p><d2d:Content /></p></d2d:Field>
                </span>
              </div>
            </article>
          </d2d:Template>
      ]]>
    </d2d:hasTemplate>


  </d2d:ArticleRenderDef>


  <d2d:ArticleRenderDef rdf:about="#Right1Col">
